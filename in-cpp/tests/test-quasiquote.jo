(assert (eq (quasiquote (0 1 2 3))
            (* 0 1 2 3)))
(assert (eq (quasiquote (0 1 (unquote (add 1 1)) 3))
            (* 0 1 2 3)))
(assert (eq (quasiquote (0 (unquote-splicing (* 1 2)) 3))
            (* 0 1 2 3)))

(assert (eq `(0 1 2 3)
            (* 0 1 2 3)))
(assert (eq `(0 1 ~(add 1 1) 3)
            (* 0 1 2 3)))
(assert (eq `(0 ~@(* 1 2) 3)
            (* 0 1 2 3)))
